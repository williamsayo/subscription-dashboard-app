{
    "version": 2,
    "functions": {
        "api/index.js": {
            "includeFiles": ["template/email_notification.hbs"]
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "crons": [
        {
            "path": "/notification/email/send-reminders",
            "schedule": "0 0 */7 * *"
        }
    ]
}
