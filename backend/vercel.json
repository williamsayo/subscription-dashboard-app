{
    "version": 2,
    "builds": [{ "src": "index.js", "use": "@vercel/node" }],
    "functions": {
        "index.js": {
            "includeFiles": "template/email_notification.hbs"
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "crons": [
        {
            "path": "/notification/email/send-reminders",
            "schedule": "0 0 */7 * *"
        }
    ]
}
